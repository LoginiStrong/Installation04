function main params endparams
float $x
float $y
float $r
float $g
float $b
float $sx
float $sy
= $x 20
= $y 40
= $r 80
= $g -30
= $b 50
= $sx 30
= $sy 30
callfunction testop args $r $g endargs
callfunction drawsquare args $r $g $b $x $y $sx $sy endargs
endfunction

function testop params $p1 $p2 endparams
print $p1
print $p2
endfunction